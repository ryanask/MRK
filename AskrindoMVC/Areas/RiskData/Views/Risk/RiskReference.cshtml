@model IEnumerable<AskrindoMVC.Models.RiskEvent>

@{
    ViewBag.Title = "RiskReference";
    Layout = "~/Areas/RiskData/Views/Shared/_RiskReference.cshtml";
}

<table class="list">
    <tr>
        <th>ID</th>
        <th>Peristiwa Risiko</th>
        <th colspan=2>Proses</th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@Html.DisplayFor(modelItem => item.RiskEventID)</td>
        <td>@Html.DisplayFor(modelItem => item.RiskEvent1)</td>
        <td>@Html.ActionLink("Edit","Edit")</td>
        <td>@Html.ActionLink("Delete","Delete")</td>
    </tr>
    }
</table>

<div style="padding-top: 12px">
    <input type="Button" value="Tambah"/>
</div>
